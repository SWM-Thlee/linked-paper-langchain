{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "lambda:InvokeFunction",
        "Resource": "arn:aws:lambda:ap-northeast-2:058264275251:function:*"
      },
      {
        "Effect": "Allow",
        "Action": [
          "logs:CreateLogGroup",
          "logs:CreateLogStream",
          "logs:PutLogEvents"
        ],
        "Resource": [
          "arn:aws:logs:ap-northeast-2:058264275251:log-group:/aws/lambda/*:*",
          "arn:aws:logs:ap-northeast-2:058264275251:log-group:/aws/lambda/*:log-stream:*"
        ]
      },
      {
        "Effect": "Allow",
        "Action": [
            "s3:PutObject",
            "s3:GetObject",
            "s3:DeleteObject",
            "s3:ListBucket"
        ],
        "Resource": [
            "arn:aws:s3:::paper-feed",
            "arn:aws:s3:::paper-feed/*"
        ]
      }
    ]
  }
